# Ignore node_modules in both backend and frontend
/backend/node_modules/
/frontend/node_modules/

# Ignore build/dist folders
/backend/dist/
/frontend/dist/
/backend/build/
/frontend/build/

# Ignore environment variable files
/backend/.env
/frontend/.env

# Ignore logs
*.log
/backend/*.log
/frontend/*.log

# Ignore OS files
.DS_Store
Thumbs.db

# Ignore editor config files
.vscode/
/.idea/

# Ignore npm-debug logs
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore lock files if you want (optional)
/backend/package-lock.json
/frontend/package-lock.json

# Ignore generated files
/backend/*.tsbuildinfo
/frontend/*.tsbuildinfo
